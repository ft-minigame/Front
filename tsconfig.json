{
  "compilerOptions": {
  "skipLibCheck" : true,
  "allowJs": true,
  "jsx": "react",
  "strict": true,
  "module": "commonjs",
  "moduleResolution": "node",
  "lib": ["es6", "dom"],
  "target": "ES5",
  "outDir": "./dist",
  "esModuleInterop": true,
  "typeRoots": ["node_modules/@types", "src/types"]
  },
  "exclude": ["dist", "build", "node_modules"],
  "include": ["src", "src/assets"]
}
